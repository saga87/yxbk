<?xml version="1.0" encoding="UTF-8"?>

<ehcache> 
    <diskStore path="java.io.tmpdir"/> 
    <defaultCache 
           maxElementsInMemory="1000" 
           eternal="false" 
           timeToIdleSeconds="1" 
           timeToLiveSeconds="1" 
           overflowToDisk="false"/> 
   
    <cache name="zxhCache" 
           maxElementsOnDisk="20000" 
           maxElementsInMemory="2000" 
           eternal="true" 
           overflowToDisk="true" 
           diskPersistent="true"
           memoryStoreEvictionPolicy="LFU"
           /> 
</ehcache> 
